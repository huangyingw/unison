[remote "bcpierce00"]
    url = git@github.com:bcpierce00/unison.git
    fetch = +refs/heads/*:refs/remotes/bcpierce00/*
[remote "github"]
    url = git@github.com:huangyingw/unison.git
    fetch = +refs/heads/*:refs/remotes/github/*
[push]
    remote = github
[gsync]
    remote = bcpierce00
    branch = master
[merge]
	allow = true
[branch "dev"]
    remote = github
    merge = refs/heads/dev
[branch "master"]
    remote = bcpierce00
    merge = refs/heads/master
[commit]
	message = n
